local page3 = UI:CreatePage("Credits") -- creates a page
local section9 = page3:CreateSection(".") -- creates a section in page with the name passed. This argument is reqiured.

section9:CreateToggle({ -- IMPORTANT: This function does not return anything, please modify flags directly in order to read or update toggle values. SCROLL TO BOTTOM OF PAGE TO SEE HOW TO MODIFY FLAGS
    Name = "Buff's Discord server & Youtube"; -- required: name of element
    Flag = "MyToggle"; -- required: unique flag name to use
    Default = true; -- optional: default value for toggle, will be used if config saving is disabled and there is no saved data, will be false if left nil
    Callback = function() -- optional: function that will be called when toggled, it is reccomended to modify flags directly
        setclipboard("https://bit.ly/BuffyBIO")
            game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Sub To Buffalo"; Text = "Successful! Coppied \n Join Discord and Youtube for support"; Icon = "rbxassetid://9327507243"; Duration = 10 })
    end;
})

local page4 = UI:CreatePage("NEWS") -- creates a page
local section4 = page4:CreateSection("The owner of the best sl2 hub :") -- creates a section in page with the name passed. This argument is reqiured.
local MyParagraph = section4:CreateParagraph("Today's news") -- creates a paragraph element with "Hello world!" as the text content
MyParagraph.Set("I am preparing for a big update and solar hub will be free forever.") -- updates the paragraph element with new text. Note how you do not use namecalling (MyParagraph:Set) and instead we use MyParagraph.Set

local section5 = page4:CreateSection("Yourdumbskid :") -- creates a section in page with the name passed. This argument is reqiured.
local MyParagraph = section5:CreateParagraph("Today's news") -- creates a paragraph element with "Hello world!" as the text content
MyParagraph.Set("#RoadTo10k") -- updates the paragraph element with new text. Note how you do not use namecalling (MyParagraph:Set) and instead we use MyParagraph.Set

local section6 = page4:CreateSection("Buffalo :") -- creates a section in page with the name passed. This argument is reqiured.
local MyParagraph = section6:CreateParagraph("Today's news") -- creates a paragraph element with "Hello world!" as the text content
MyParagraph.Set("im doing everything rn for 1k #RoadTo1k") -- updates the paragraph element with new text. Note how you do not use namecalling (MyParagraph:Set) and instead we use MyParagraph.Set
