-- Made by Buff

local Atlas = loadstring(game:HttpGet("https://rd2glory.com/Atlas.lua"))()

local UI = Atlas.new({
    Name = "SL2 FreeHubX"; -- script name (required)
    ConfigFolder = "MyScript"; -- folder name to save configs to, set this to nil if you want to disable configs saving
    Credit = "Made By Buffalo#2222"; -- text to appear if player presses the "i" button on their UI (optional)
    Color = Color3.fromRGB(51,153,255); -- theme color for UI (required)
    Bind = "LeftControl"; -- keybind for toggling the UI, must be a valid KeyCode name
    -- Atlas Loader:
    UseLoader = true; -- whether to use Atlas Loader or not, if set to false than CheckKey and Discord are ignored
    FullName = "SL2 FreeHubX"; -- works if UseLoader is set to true, can be same as Name argument, shown on loader frame
    CheckKey = function(inputtedKey) -- works if UseLoader is set to true, where inputtedKey is the key inputted by the player
        return inputtedKey=="roadto1k"
    end;
    Discord = "https://discord.gg/u6PWdXmQZG"; -- works if UseLoader is set to true, will be copied to clipboard if player presses "Copy Invite" button
})
UI:Notify({
    Title = "Welcome to BuffHub";
    Content = "SUBSCRIBE TO BUFFALO ON YT";
  })
local page = UI:CreatePage("Main Page") -- creates a page

local section = page:CreateSection("6 packs") -- creates a section in page with the name passed. This argument is reqiured.

section:CreateInteractable({
    Name = "Solar Hub"; -- required: name of element
    ActionText = "Execute"; -- required: text in the interactable button
    Callback = function() -- function to be called when the interactable is activated
        loadstring(game:HttpGet("https://raw.githubusercontent.com/rip2point/Solar-Hub/main/Other%20Stuff/SOLARHUB", true))()
    end;
})

local section2 = page:CreateSection("5 packs") -- creates a section in page with the name passed. This argument is reqiured.
section2:CreateInteractable({
    Name = "Horizon (Baby ReaperHub)"; -- required: name of element
    ActionText = "Execute"; -- required: text in the interactable button
    Callback = function() -- function to be called when the interactable is activated
        loadstring(game:HttpGet("https://raw.githubusercontent.com/MrBuff2/horizon/main/sl2"))()
    end;
})
local section3 = page:CreateSection("3 packs") -- creates a section in page with the name passed. This argument is reqiured.
section3:CreateInteractable({
    Name = "Buff Hub"; -- required: name of element
    ActionText = "Execute"; -- required: text in the interactable button
    Callback = function() -- function to be called when the interactable is activated
        loadstring(game:HttpGet(("https://raw.githubusercontent.com/MrBuff2/MrBuff/main/BuffHubSL2"), true))()
    end;
})

local section4 = page:CreateSection("Fat Belly") -- creates a section in page with the name passed. This argument is reqiured.
section4:CreateInteractable({
    Name = "1oftheoldhub"; -- required: name of element
    ActionText = "Execute"; -- required: text in the interactable button
    Callback = function() -- function to be called when the interactable is activated

    end;
})

local page2 = UI:CreatePage("Op Page") -- creates a page

local section10 = page2:CreateSection(".") -- creates a section in page with the name passed. This argument is reqiured.

section10:CreateInteractable({
    Name = "Cash Sender"; -- required: name of element
    ActionText = "Execute"; -- required: text in the interactable button
    Callback = function() -- function to be called when the interactable is activated
        loadstring(game:HttpGet("https://raw.githubusercontent.com/MonkeyHUBB/cashsender/main/README.md"))()
